    "framer-motion": "^12.4.10",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "grpc-web": "^2.0.1",
    "gsap": "^3.13.0",
    "immer": "^10.1.3",
    "isomorphic-dompurify": "^2.26.0",
    "jose": "^6.0.11",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "katex": "^0.16.22",
    "keen-slider": "^6.8.6",
    "lucide-react": "^0.352.0",
    "monaco-editor": "^0.52.2",
    "next": "15.4.5",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "nuqs": "^1.17.0",
    "prismjs": "^1.30.0",
    "react": "19.1.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.2",
    "sharp": "^0.34.3",
    "socket.io-client": "^4.7.0",
    "sonner": "^2.0.7",
    "styled-jsx": "^5.1.1",
    "tailwind-merge": "^2.6.0",
