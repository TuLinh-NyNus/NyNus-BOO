{
  "name": "@nynus/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:verbose": "next dev -p 3000",
    "dev:fast": "next dev -p 3000 --turbo",
    "dev:debug": "cross-env NODE_OPTIONS=--inspect next dev -p 3000",
    "dev:analyze": "cross-env ANALYZE=true next dev -p 3000",
    "build": "pnpm build:search-index && next build",
    "build:turbo": "pnpm build:search-index && next build --turbo",
    "build:fast": "pnpm build:search-index && next build --turbo --no-lint",
    "build:analyze": "cross-env ANALYZE=true pnpm build:search-index && next build",
    "build:search-index": "node scripts/generate-search-index.js",
    "start": "next start",
    "start:fast": "next start --turbo",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fast": "next lint --max-warnings 0 --quiet",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:fast": "tsc --noEmit --skipLibCheck",
    "lint:contexts": "eslint \"src/contexts/**/*.{ts,tsx}\" --format=compact --max-warnings=0",
    "lint:hooks": "eslint \"src/hooks/**/*.{ts,tsx}\" --format=compact --max-warnings=0",
    "lint:api": "eslint \"src/app/api/**/*.ts\" --format=compact --max-warnings=0",
    "lint:stores": "eslint \"src/store/**/*.ts\" --format=compact --max-warnings=0",
    "lint:components": "eslint \"src/components/**/*.{ts,tsx}\" --format=compact --max-warnings=0",
    "lint:lib": "eslint \"src/lib/**/*.ts\" --format=compact --max-warnings=0",
    "clean": "rm -rf .next ; rm -rf node_modules ; rm -rf tsconfig.tsbuildinfo",
    "clean:cache": "rm -rf .next/cache",
    "clean:all": "pnpm clean && pnpm clean:cache",
    "dev:clean": "pnpm clean:cache && pnpm dev:fast",
    "perf:dev": "pnpm clean:cache && time pnpm dev:fast",
    "perf:build": "pnpm clean:cache && time pnpm build:fast",
    "perf:type-check": "time pnpm type-check:fast",
    "cache:clean": "node scripts/dev/cache-manager.js clean",
    "cache:optimize": "node scripts/dev/cache-manager.js optimize",
    "cache:status": "node scripts/dev/cache-manager.js status",
    "perf:monitor": "node scripts/dev/performance-monitor.js",
    "perf:report": "node scripts/dev/performance-monitor.js report",
    "perf:clear": "node scripts/dev/performance-monitor.js clear",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit/frontend",
    "test:integration": "jest --config jest.integration.config.js",
    "test:auth": "node scripts/test-auth-integration.js",
    "test:auth-only": "jest --config jest.integration.config.js src/tests/integration/auth-integration.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration-coverage": "jest --config jest.integration.config.js --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
